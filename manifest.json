{
  "manifest_version": 3,
  "name": "Custom Console Runner",
  "version": "1.0",
  "description": "Run custom JavaScript on the current tab from a popup.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
} 